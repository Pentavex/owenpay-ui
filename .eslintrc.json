{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "comma-dangle": 0,
        "react/destructuring-assignment": [true, "always", { "ignoreClassFields": true }]
      },
      "parser": "babel-eslint",
      "env": {
          "browser": true
      }
}